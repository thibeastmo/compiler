{
	"name": "antlr-java-project",
	"version": "1.0.0",
	"description": "ANTLR Java Project",
	"main": "index.js",
	"scripts": {
		"antlr": "java -cp lib/antlr-4.9.3-complete.jar org.antlr.v4.Tool",
		"antlr-gen": "java -cp lib/antlr-4.9.3-complete.jar org.antlr.v4.Tool -visitor src/antlr/BasicJava.g4",
		"java-gen": "javac -cp \".;antlr-4.9.3-complete.jar\" BasicJava*.java",
		"grun-gui": "java -cp \\\".;antlr-4.9.3-complete.jar\\\" org.antlr.v4.gui.TestRig BasicJava prog -gui src/app/tests/test0.txt",
		"grun": "java -cp \".;antlr-4.9.3-complete.jar\" org.antlr.v4.gui.TestRig",
		"antlr-compile": "lib/antlr-4.9.3-complete.jar",
		"gen-compile-debug": "npm run antlr-gen && npm run java-gen && npm run grun-gui",
		"gen-compile": "npm run antlr-gen && npm run java-gen",
		"test": "java -cp \\\".;antlr-4.9.3-complete.jar\\\" src/app/tests/test0.txt",
		"gen-jgen-test": "npm run antlr-gen && npm run java-gen && npm run test",
		"output": "java -cp \".;antlr-4.9.3-complete.jar\" BasicJavaDriver",
		"gen-compile-output": "npm run gen-compile && npm run output",
		"javac-main": "javac -cp \".;antlr-4.9.3-complete.jar\" Main.java"
	},
	"author": "Thimo Mortelmans",
	"license": "MIT",
	"devDependencies": {
	  "antlr4": "^4.9.3"
	}
}