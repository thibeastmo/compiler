{
	"name": "antlr-java-project",
	"version": "1.0.0",
	"description": "ANTLR Java Project",
	"main": "index.js",
	"scripts": {
		"antlr": "java -cp antlr-4.9.3-complete.jar org.antlr.v4.Tool",
		"antlr-gen": "java -cp antlr-4.9.3-complete.jar org.antlr.v4.Tool BasicJava.g4",
		"java-gen": "javac -cp \".;antlr-4.9.3-complete.jar\" BasicJava*.java",
		"grun-gui": "java -cp \".;antlr-4.9.3-complete.jar\" org.antlr.v4.gui.TestRig BasicJava program -gui test0.txt",
		"grun": "java -cp \".;antlr-4.9.3-complete.jar\" org.antlr.v4.gui.TestRig",
		"antlr-compile": "antlr-4.9.3-complete.jar",
		"gen-compile-debug": "npm run antlr-gen && npm run java-gen && npm run grun-gui",
		"gen-compile": "npm run antlr-gen && npm run java-gen",
		"test": "java -cp \".;antlr-4.9.3-complete.jar\" test0.txt"
	},
	"author": "Thimo Mortelmans",
	"license": "MIT",
	"devDependencies": {
	  "antlr4": "^4.9.3"
	}
}